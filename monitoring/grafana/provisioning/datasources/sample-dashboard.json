{
"dashboard": {
"id": null,
"uid": "api-metrics",
"title": "API Metrics",
"timezone": "browser",
"schemaVersion": 37,
"version": 1,
"refresh": "5s",
"panels": [
{
"type": "timeseries",
"title": "API Request Rate",
"datasource": "Prometheus",
"targets": [
{
"expr": "rate(api_request_count_total[1m])",
"legendFormat": "requests/sec"
}
],
"gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
},
{
"type": "timeseries",
"title": "API Latency (p50)",
"datasource": "Prometheus",
"targets": [
{
"expr": "histogram_quantile(0.5, sum(rate(api_request_latency_seconds_bucket[1m])) by (le))",
"legendFormat": "p50 latency"
}
],
"gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
},
{
"type": "timeseries",
"title": "API Latency (p95)",
"datasource": "Prometheus",
"targets": [
{
"expr": "histogram_quantile(0.95, sum(rate(api_request_latency_seconds_bucket[1m])) by (le))",
"legendFormat": "p95 latency"
}
],
"gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 }
}
]
},
"overwrite": true
}